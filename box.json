{
    "output": "build/melody.phar",
    "chmod": "0755",
    "compactors": [
        "Herrera\\Box\\Compactor\\Php"
    ],
    "compression": "GZ",
    "directories": ["src"],
    "files": [
        "LICENSE"
    ],
    "finder": [
        {
            "name": ["*.php"],
            "exclude": [ "Tests", "tests" ],
            "in": ["src", "vendor"]
        }
    ],
    "git-commit": "git-commit",
    "main": "bin/melody",
    "stub": true
}
